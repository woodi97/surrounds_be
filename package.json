{
  "name": "surrounds-be",
  "version": "0.0.1",
  "description": "backend repository for surrounds",
  "author": "woodi",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "prestart:prod": "rimraf dist && yarn build",
    "web": "yarn start:prod",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "typeorm": "ts-node node_modules/typeorm/cli.js -f ormconfig",
    "migration:generate": "yarn run typeorm migration:generate -d src/database/migrations -n",
    "migration:create": "yarn run typeorm migration:create -d src/database/migrations -n",
    "migration:revert": "yarn run typeorm migration:revert",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.3",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/platform-socket.io": "^9.0.4",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/typeorm": "8.0.2",
    "@nestjs/websockets": "^9.0.4",
    "@socket.io/redis-adapter": "^7.2.0",
    "@types/webpack-env": "^1.17.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express-ctx": "^0.1.1",
    "express-rate-limit": "^6.4.0",
    "geojson": "^0.5.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.1",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "peer": "^0.6.1",
    "pg": "^8.7.3",
    "qs": "^6.11.0",
    "redis": "^4.2.0",
    "redix": "^1.0.22",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "0.2.45",
    "uuid": "^8.3.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@moneteam/eslint-plugin-nestjs": "^1.6.0",
    "@nestjs/testing": "^9.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^28.1.6",
    "@types/lodash-es": "^4.17.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.5",
    "@types/passport": "^1.0.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-kakao": "^1.0.0",
    "@types/passport-local": "^1.0.34",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    }
  }
}
